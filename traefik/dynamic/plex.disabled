# Plex Media Server (example)
http:
  routers:
    plex:
      rule: "Host(`plex.octopod.eu`)"
      entryPoints:
        - websecure
      service: plex
      middlewares:
        - authelia@docker
      tls:
        certResolver: letsencrypt

  services:
    plex:
      loadBalancer:
        servers:
          - url: "http://192.168.1.102:32400"
